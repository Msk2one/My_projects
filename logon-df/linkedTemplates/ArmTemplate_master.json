{
	"$schema": "http://schema.management.azure.com/schemas/2015-01-01/deploymentTemplate.json#",
	"contentVersion": "1.0.0.0",
	"parameters": {
		"factoryName": {
			"type": "string",
			"metadata": "Data Factory name",
			"defaultValue": "logon-df"
		},
		"sink_ls_accountKey": {
			"type": "secureString",
			"metadata": "Secure string for 'accountKey' of 'sink_ls'"
		},
		"source_ls_1_connectionString": {
			"type": "secureString",
			"metadata": "Secure string for 'connectionString' of 'source_ls_1'"
		},
		"sink_ls_properties_typeProperties_url": {
			"type": "string",
			"defaultValue": "https://logonstorageac.dfs.core.windows.net/"
		},
		"source_ls_properties_typeProperties_server": {
			"type": "string",
			"defaultValue": "@{linkedService().serverName}"
		},
		"source_ls_properties_typeProperties_database": {
			"type": "string",
			"defaultValue": "@{linkedService().dbName}"
		},
		"source_ls_properties_typeProperties_userName": {
			"type": "string",
			"defaultValue": "@{linkedService().userNmae}"
		},
		"new csv file to json_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/81832be2-97b9-46c7-bf8d-6d2daec9fdd5/resourceGroups/msk_rg/providers/Microsoft.Storage/storageAccounts/logonstorageac"
		},
		"trigger1_properties_typeProperties_scope": {
			"type": "string",
			"defaultValue": "/subscriptions/81832be2-97b9-46c7-bf8d-6d2daec9fdd5/resourceGroups/msk_rg/providers/Microsoft.Storage/storageAccounts/logonstorageac"
		},
		"containerUri": {
			"type": "string",
			"metadata": "URI to storage account containing linked ARM templates"
		},
		"containerSasToken": {
			"type": "string",
			"metadata": "Sas token to storage account containing linked ARM templates"
		}
	},
	"variables": {
		"factoryId": "[concat('Microsoft.DataFactory/factories/', parameters('factoryName'))]"
	},
	"resources": [
		{
			"name": "logon-df_ArmTemplate_0",
			"type": "Microsoft.Resources/deployments",
			"apiVersion": "2024-03-01",
			"properties": {
				"mode": "incremental",
				"templateLink": {
					"uri": "[concat(parameters('containerUri'), '/ArmTemplate_0.json', parameters('containerSasToken'))]",
					"contentVersion": "1.0.0.0"
				},
				"parameters": {
					"factoryName": {
						"value": "[parameters('factoryName')]"
					},
					"sink_ls_accountKey": {
						"value": "[parameters('sink_ls_accountKey')]"
					},
					"source_ls_1_connectionString": {
						"value": "[parameters('source_ls_1_connectionString')]"
					},
					"sink_ls_properties_typeProperties_url": {
						"value": "[parameters('sink_ls_properties_typeProperties_url')]"
					},
					"source_ls_properties_typeProperties_server": {
						"value": "[parameters('source_ls_properties_typeProperties_server')]"
					},
					"source_ls_properties_typeProperties_database": {
						"value": "[parameters('source_ls_properties_typeProperties_database')]"
					},
					"source_ls_properties_typeProperties_userName": {
						"value": "[parameters('source_ls_properties_typeProperties_userName')]"
					},
					"new csv file to json_properties_typeProperties_scope": {
						"value": "[parameters('new csv file to json_properties_typeProperties_scope')]"
					},
					"trigger1_properties_typeProperties_scope": {
						"value": "[parameters('trigger1_properties_typeProperties_scope')]"
					}
				}
			},
			"dependsOn": []
		}
	]
}